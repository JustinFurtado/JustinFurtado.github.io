div.project-group.p-3.m-5
    h1.text-center.m-4.p-3.font-weight-bold #{inputData.title}
    div.m-4
        - if (inputData.imageData.linkedShow && inputData.imageData.linkedShow == "true") {
            !=partial("linkedshow", {images: inputData.imageData})
        - } else {
            !=partial("slideshow", {images: inputData.imageData})
        - }
    - for (var i = 0; i < inputData.numParagraphs; i++){
    p.m-3.text-justify #{inputData.descriptions[i]}
    - }
    - if (inputData.numCredits != undefined) {
    h4.m-3.font-weight-bold.text-left Project Credits
    ul.m-3
        li
            p.m-3.text-justify 
                | Justin Furtado (Me) - 
                a(href="#{baseUrl}/projects.html") More Projects By Me
        - for (var i = 0; i < inputData.numCredits; i++){
        li
            p.m-3.text-justify
                | #{inputData.credits[i].prefix}
                a(href=inputData.credits[i].link) #{inputData.credits[i].postfix}
        - }
    - }